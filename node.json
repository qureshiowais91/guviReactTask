Q10 

db.collection.aggregate([
    {
      $group: {
        _id: "$product_price",
        count: {
          $sum: 1
        },
        ids: {
          $push: "$_id"
        }
      }
    },
    {
      "$match": {
        count: {
          $gte: 2
        }
      }
    },
    {
      $unwind: "$ids"
    },
    {
      $project: {
        _id: "$ids"
      }
    },
    
  ]).forEach(function(doc) {
    db.products.deleteOne({ _id: doc._id });
  });
